{
  "kind": "collectionType",
  "collectionName": "documents",
  "info": {
    "name": "Document"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "documents": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
